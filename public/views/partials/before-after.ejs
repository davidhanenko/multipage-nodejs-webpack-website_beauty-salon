<%% if(service.imageBeforeAfter.length> 0) { %>

  <div class="row align-items-center pb-4 before-after-block">
    <div class="col d-flex justify-content-center">
      <h2>Before vs After</h2>
    </div>
  </div>
  <div class="row align-items-center pt-3 pb-5">
    <div class="col-xs-10 offset-xs-1 col-md-8 offset-md-2 d-flex justify-content-center">

      <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
        <div class="carousel-inner">

          <%% service.imageBeforeAfter.forEach( img=> { %>
            <div class="carousel-item" id="carousel-toggleclass">
              <div class="row no-gutters">
                <div class="col-6"><img src="<%%= img.imageBefore.url %>" class="image-before-after"
                    alt="<%%= service.title %>">
                </div>
                <div class="col-6"><img src="<%%= img.imageAfter.url %>" class="image-before-after"
                    alt="<%%= service.title %>">
                </div>
              </div>


            </div>
            <%% }) %>


        </div>
        <%% if(service.imageBeforeAfter.length> 1) { %>
          <a class="carousel-control-prev" href="#carouselExampleFade" role="button" data-slide="prev">
            <span class="arrow-left" aria-hidden="true"><img src="../../imgs/chevron-left.svg" alt=""></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleFade" role="button" data-slide="next">
            <span class="arrow-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>

          <%% } %>
      </div>

      <!-- arrow -->
      <!-- <div class="arrow">
      <span></span>
      <span></span>
      <span></span>
    </div> -->

    </div>
  </div>

  <%% } %>